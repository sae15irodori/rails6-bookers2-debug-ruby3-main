<div class="serch_form text-center mt-3">
    <%= form_with url: search_path,method: :get, local: true do |f| %>
    <%= f.text_field :content %><!--ﾕｰｻﾞｰがが入力したものがcontentとしてｱｸｼｮﾝに送られる-->
    <%= f.select :model,options_for_select({"User"=>"user","Book"=>"book"})%><!--f.selectプルダウン-->
    <%= f.select :method,options_for_select({"完全一致"=>"perfect","前方一致"=>"forward",
        "後方一致"=>"backward","部分一致"=>"partial"})%>
    <%= f.submit'検索' %>
    <% end %>
</div>